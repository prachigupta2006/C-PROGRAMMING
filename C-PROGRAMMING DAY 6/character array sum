#include <stdio.h>

int main()
{
    int arr[] = {10,21,30,41,50,61,70,81,90,101};
    for(int i = 0;i<=9 ;i++)
    {
        printf("%d ", arr[i]);
    }    
    for (int i = 0;i<=9;i++)
    {
        if (arr[i]%2 == 0) //even
        {
            arr[i]=arr[i]*5;
        }
        else
        {
            arr[i] = arr[i] + 1;
        }
    }
        printf("\nupdated\n");
        for(int i =0;i<=9;i++)
        {
            printf("%d ", arr[i]);
        }
        
    return 0;
}    