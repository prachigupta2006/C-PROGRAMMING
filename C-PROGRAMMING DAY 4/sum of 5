#include <stdio.h>

int main() {
    int numbers[5]; // Array to store 5 numbers
    int sum = 0;
    
    // Prompt the user to enter 5 numbers
    printf("Enter 5 numbers:\n");
    for (int i = 0; i < 5; i++) {
        printf("Number %d: ", i + 1);
        scanf("%d", &numbers[i]);
        sum += numbers[i]; // Accumulate the sum
    }
    
    // Print the sum
    printf("The sum of the 5 numbers is %d\n", sum);
    
    return 0;
}